{
  "manifest_version": 3,
  "name": "Tab Title Viewer",
  "version": "1.0",
  "description": "Get the title of the current tab using React + Tailwind",
  "permissions": ["tabs", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
